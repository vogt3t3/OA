<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="tb_workFlow" name="com.bf.po.workFlow.WorkFlow">
    <id name="wf_id">
      <generator class="native"/>
    </id>
    <property name="wf_name"/>
    <property name="processFile" type="binary" length="9999999"/>
     <property name="processImg" type="binary" length="9999999"/>
    <property name="flag"/>   
  </class>
</hibernate-mapping>
