<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<package name="bfmod" namespace="/" extends="struts-default">
        <action name="mod_*" class="modAction" method="{1}">
            <result name="index">/web/page/mod/showMod.jsp
            </result>
             <result name="openAddMod">/web/page/mod/addMod.jsp
            </result>
            <result name="addMod">/web/page/common/close.html
            </result>
            <result name="findParentMod">/web/page/main/main_left.jsp
            </result>
             <result name="findSubMod">/web/page/main/main_right.jsp
            </result>
           
        </action>
</package>
</struts>
